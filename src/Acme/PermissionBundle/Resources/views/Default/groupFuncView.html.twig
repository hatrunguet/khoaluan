{% for g in gfunc%}
    <li>
       <a class="expand">{{g.name}}<span></span></a>
            <ul>
                {% for f in g.functions %}
                <li>
                   <a href="{{path(f.path)}}" class="expand"><span>{{f.name}}</span></a>
                </li>
                {% endfor%}
            </ul>
    </li>    
{% endfor%}

